-- JONHSCRIPTS BROOKHAVEN GUI (LIMPIO + Velocity Ajustable + Toggle Bola Azul)

local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")

-- CREACI칍N DE GUI (tu dise침o exportado de Gui2Lua)
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local TextLabel = Instance.new("TextLabel")
local JumpButton = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local VelocityButton = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local VelocityButton_2 = Instance.new("TextButton") -- -
local UICorner_9 = Instance.new("UICorner")
local VelocityButton_3 = Instance.new("TextButton") -- +
local UICorner_10 = Instance.new("UICorner")
local MedidorLabel = Instance.new("TextLabel")
local UICorner_11 = Instance.new("UICorner")

-- ScreenGui
ScreenGui.Parent = Player:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Frame
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(52, 40, 61)
Frame.BackgroundTransparency = 0.2
Frame.Position = UDim2.new(0.15, 0, 0.1, 0)
Frame.Size = UDim2.new(0, 800, 0, 500)
Frame.Active = true
Frame.Draggable = true
Frame.Visible = false -- 游댮 OCULTO al inicio

UICorner.Parent = Frame
UICorner.CornerRadius = UDim.new(0, 99)

-- T칤tulo
TextLabel.Parent = Frame
TextLabel.BackgroundTransparency = 1
TextLabel.Position = UDim2.new(0.375, 0, 0, 0)
TextLabel.Size = UDim2.new(0, 200, 0, 50)
TextLabel.Font = Enum.Font.FredokaOne
TextLabel.Text = "JONHSCRIPTS BROOKHAVEN"
TextLabel.TextColor3 = Color3.new(1, 1, 1)
TextLabel.TextSize = 20

-- JumpButton
JumpButton.Parent = Frame
JumpButton.Position = UDim2.new(0.0225, 0, 0.176, 0)
JumpButton.Size = UDim2.new(0, 135, 0, 52)
JumpButton.BackgroundColor3 = Color3.new(1, 1, 1)
JumpButton.BackgroundTransparency = 0.3
JumpButton.Text = "INF JUMP"
JumpButton.Font = Enum.Font.FredokaOne
JumpButton.TextSize = 14
UICorner_2.Parent = JumpButton

-- VelocityButton
VelocityButton.Parent = Frame
VelocityButton.Position = UDim2.new(0.3975, 0, 0.176, 0)
VelocityButton.Size = UDim2.new(0, 135, 0, 52)
VelocityButton.BackgroundColor3 = Color3.new(1, 1, 1)
VelocityButton.BackgroundTransparency = 0.3
VelocityButton.Text = "Velocity"
VelocityButton.Font = Enum.Font.FredokaOne
VelocityButton.TextSize = 14
UICorner_3.Parent = VelocityButton

-- Bot칩n -
VelocityButton_2.Parent = Frame
VelocityButton_2.Position = UDim2.new(0.333, 0, 0.182, 0)
VelocityButton_2.Size = UDim2.new(0, 43, 0, 45)
VelocityButton_2.Text = "-"
UICorner_9.Parent = VelocityButton_2

-- Bot칩n +
VelocityButton_3.Parent = Frame
VelocityButton_3.Position = UDim2.new(0.578, 0, 0.182, 0)
VelocityButton_3.Size = UDim2.new(0, 43, 0, 45)
VelocityButton_3.Text = "+"
UICorner_10.Parent = VelocityButton_3

-- MedidorLabel
MedidorLabel.Parent = ScreenGui
MedidorLabel.Position = UDim2.new(0.45, 0, 0.29, 0)
MedidorLabel.Size = UDim2.new(0, 104, 0, 34)
MedidorLabel.BackgroundColor3 = Color3.new(1, 1, 1)
MedidorLabel.BackgroundTransparency = 0.3
MedidorLabel.Text = "0" -- 游댮 ahora s칤 arranca en 0
MedidorLabel.TextSize = 14
UICorner_11.Parent = MedidorLabel

-- 游댯 Bola Toggle
local ToggleBall = Instance.new("Frame")
ToggleBall.Parent = ScreenGui
ToggleBall.Size = UDim2.new(0, 50, 0, 50)
ToggleBall.Position = UDim2.new(0.05, 0, 0.5, 0)
ToggleBall.BackgroundColor3 = Color3.fromRGB(0, 102, 255)
ToggleBall.Active = true
ToggleBall.Draggable = true

local BallCorner = Instance.new("UICorner")
BallCorner.CornerRadius = UDim.new(1, 0)
BallCorner.Parent = ToggleBall

ToggleBall.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		Frame.Visible = not Frame.Visible
	end
end)

-------------------------------------------------------
-- FUNCIONES
-------------------------------------------------------

-- INF JUMP
local infJumpEnabled = false
JumpButton.MouseButton1Click:Connect(function()
	infJumpEnabled = not infJumpEnabled
	JumpButton.Text = infJumpEnabled and "INF JUMP [ON]" or "INF JUMP [OFF]"
end)

game:GetService("UserInputService").JumpRequest:Connect(function()
	if infJumpEnabled and Humanoid then
		Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	end
end)

-- VELOCITY
local velocityEnabled = false
local currentSpeed = 16
local defaultSpeed = 25

local function updateMedidor()
	MedidorLabel.Text = tostring(currentSpeed)
end

VelocityButton.MouseButton1Click:Connect(function()
	velocityEnabled = not velocityEnabled
	if velocityEnabled then
		currentSpeed = defaultSpeed
		Humanoid.WalkSpeed = currentSpeed
		VelocityButton.Text = "Velocity [ON]"
	else
		currentSpeed = 16
		Humanoid.WalkSpeed = currentSpeed
		VelocityButton.Text = "Velocity [OFF]"
	end
	updateMedidor()
end)

VelocityButton_3.MouseButton1Click:Connect(function()
	if velocityEnabled then
		currentSpeed = currentSpeed + 5
		Humanoid.WalkSpeed = currentSpeed
		updateMedidor()
	end
end)

VelocityButton_2.MouseButton1Click:Connect(function()
	if velocityEnabled and currentSpeed > 5 then
		currentSpeed = currentSpeed - 5
		Humanoid.WalkSpeed = currentSpeed
		updateMedidor()
	end
end)

-- Inicializaci칩n
updateMedidor()
